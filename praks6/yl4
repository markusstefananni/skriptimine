   #!/bin/bash
   # Skript, mis tuvastab, kas 20 esimest t채isarvu on algarvud ja k체sib kasutajalt arvu

   # Funktsioon, mis kontrollib, kas arv on algarv
   check_prime() {
       local number=$1
       if [ $number -le 1 ]; then
           echo "$number - ei ole algarv"
           return
       fi

       is_prime=1  # Alguses eeldame, et arv on algarv
       for (( i=2; i*i<=number; i++ )); do
           if (( number % i == 0 )); then
               is_prime=0  # Arv ei ole algarv
               break
           fi
       done

       if [ $is_prime -eq 1 ]; then
           echo "$number - on algarv"
       else
           echo "$number - ei ole algarv"
       fi
   }

   # Prindime 20 esimest t채isarvu ja kontrollime, kas need on algarvud
   for number in {1..20}; do
       check_prime $number
   done

   # K체sime kasutajalt arvu
   read -p "Sisesta suvaline arv: " user_number
   check_prime $user_number
